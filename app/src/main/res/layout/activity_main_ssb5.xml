<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivitySSB5">


    <EditText
        android:id="@+id/idEditTextSSB5"
        android:layout_width="300dp"
        android:layout_height="75dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:autofillHints="@string/idHintSSB5"
        android:hint="@string/idHintSSB5"
        android:inputType="text"
        android:selectAllOnFocus="false"
        android:singleLine="true"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
    </EditText>

    <TextView
        android:id="@+id/idScoreSSB5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        android:text="@string/idScoreSSB5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/idEditTextSSB5"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/idInkling"
        android:layout_width="150dp"
        android:layout_height="35dp"
        android:layout_marginStart="55dp"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:text="@string/idInkling"
        android:textAlignment="center"
        android:textSize="20sp"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/idEditTextSSB5"
        tools:visibility="invisible" />

    <TextView
        android:id="@+id/idDaisy"
        android:layout_width="150dp"
        android:layout_height="35dp"
        android:layout_marginStart="55dp"
        android:gravity="center"
        android:text="@string/idDaisy"
        android:textAlignment="center"
        android:textSize="20sp"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/idInkling"
        tools:visibility="invisible" />

    <TextView
        android:id="@+id/idRidley"
        android:layout_width="150dp"
        android:layout_height="35dp"
        android:layout_marginStart="55dp"
        android:gravity="center"
        android:text="@string/idRidley"
        android:textAlignment="center"
        android:textSize="20sp"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/idDaisy"
        tools:visibility="invisible" />

    <TextView
        android:id="@+id/idSimon"
        android:layout_width="150dp"
        android:layout_height="35dp"
        android:layout_marginStart="55dp"
        android:gravity="center"
        android:text="@string/idSimon"
        android:textAlignment="center"
        android:textSize="20sp"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/idRidley"
        tools:visibility="invisible" />

    <TextView
        android:id="@+id/idRichter"
        android:layout_width="150dp"
        android:layout_height="35dp"
        android:layout_marginStart="55dp"
        android:gravity="center"
        android:text="@string/idRichter"
        android:textAlignment="center"
        android:textSize="20sp"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/idSimon"
        tools:visibility="invisible" />

    <TextView
        android:id="@+id/idChrom"
        android:layout_width="150dp"
        android:layout_height="35dp"
        android:layout_marginStart="55dp"
        android:gravity="center"
        android:text="@string/idChrom"
        android:textAlignment="center"
        android:textSize="20sp"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/idRichter"
