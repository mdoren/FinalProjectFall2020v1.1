        android:text="@string/idSteve"
        android:textAlignment="center"
        android:textSize="20sp"
        android:visibility="invisible"
        app:layout_constraintStart_toEndOf="@+id/idIsabelle"
        app:layout_constraintTop_toBottomOf="@+id/idMinMin"
        tools:visibility="invisible" />

    <TextView
        android:id="@+id/idSephiroth"
        android:layout_width="150dp"
        android:layout_height="35dp"
        android:gravity="center"
        android:text="@string/idSephiroth"
        android:textAlignment="center"
        android:textSize="20sp"
        android:visibility="invisible"
        app:layout_constraintStart_toEndOf="@+id/idKen"
        app:layout_constraintTop_toBottomOf="@+id/idSteve"
        tools:visibility="invisible" />

    <Button
        android:id="@+id/idButtonCheckSSB5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="20dp"
        android:text="@string/idButtonCheckSSB5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/idEditTextSSB5"
        app:layout_constraintTop_toBottomOf="@+id/idScoreSSB5" />
