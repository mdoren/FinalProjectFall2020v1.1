        app:layout_constraintTop_toBottomOf="@+id/idTerry"
        tools:visibility="invisible" />

    <TextView
        android:id="@+id/idMinMin"
        android:layout_width="150dp"
        android:layout_height="35dp"
        android:gravity="center"
